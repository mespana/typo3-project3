{
    "name": "gowords/vacation-rental",
    "version": "1.0.0",
    "type": "typo3-cms-extension",
    "description": "Sitepackage Perkeo Apartments",
    "license": ["GPL-2.0-or-later"],
    "require": {
        "bk2k/bootstrap-package": "^12.0"
    },
    "replace": {
        "typo3-ter/vacation-rental": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Gowords\\VacationRental\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gowords\\VacationRental\\Tests\\": "Tests"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "vacation_rental",
            "web-dir": ".build/web",
            "app-dir": ".build"
        }
    }   
}

    